#to check a year is leap year or not.
import java.util.*;
class J
{
    public static void main(String args[])
    {
        Scanner sc = new Scanner(System.in);
        int a;
        a=sc.nextInt();
        if((a%400==0||a%4==0)&&a%100!=0)
            System.out.print("Leap year");
        else
            System.out.print("Not a leap year");
    }
}
